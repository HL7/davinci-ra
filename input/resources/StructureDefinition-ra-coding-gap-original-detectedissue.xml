<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="ra-coding-gap-original-detectedissue"/>
    <url value="http://hl7.org/fhir/us/davinci-ra/StructureDefinition/ra-coding-gap-original-detectedissue"/>
    <name value="RiskAdjustmentCodingGapOriginalDetectedIssue"/>
    <title value="Risk Adjustment Coding Gap Original DetectedIssue"/>
    <status value="draft"/>
    <publisher value="Da Vinci Project"/>
    <description value="This profile describes coding gaps originally identified by a payer using their data and suspecting algorithms."/>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="DetectedIssue"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/DetectedIssue"/>
    <derivation value="constraint"/>
    <differential>
        <element id="DetectedIssue">
            <path value="DetectedIssue"/>
        </element>
        <element id="DetectedIssue.meta">
            <path value="DetectedIssue.meta"/>
            <min value="1"/>
            <mustSupport value="true"/>
        </element>          
        <element id="DetectedIssue.meta">
            <path value="DetectedIssue.meta.lastUpdated"/>
            <min value="1"/>
            <max value="1"/>            
            <mustSupport value="true"/>
        </element>         
        <element id="DetectedIssue.code">
            <path value="DetectedIssue.code"/>
            <min value="1"/>
