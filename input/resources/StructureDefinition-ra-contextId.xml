      <definition value="Context id provided in a $risk-gap operation API call."/>
      <min value="0"/>
      <max value="*"/>
    </element>
    <element id="Extension.url">
      <path value="Extension.url"/>
      <type>
        <code value="uri"/>
      </type>
      <fixedUri value="http://hl7.org/fhir/us/davinci-ra/StructureDefinition/ra-contextId"/>
